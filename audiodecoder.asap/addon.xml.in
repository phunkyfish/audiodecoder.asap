<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="audiodecoder.asap"
  version="20.3.0"
  name="ASAP Audio Decoder"
  provider-name="spiff">
  <requires>@ADDON_DEPENDS@</requires>
  <extension
    point="kodi.audiodecoder"
    name="asap"
    tags="true"
    tracks="true"
    library_@PLATFORM@="@LIBRARY_FILENAME@">
    <support>
      <extension name=".sap">
        <description>30100</description>
      </extension>
      <extension name=".cmc">
        <description>30101</description>
      </extension>
      <extension name=".cmr">
        <description>30102</description>
      </extension>
      <extension name=".dmc">
        <description>30103</description>
      </extension>
      <extension name=".mpt">
        <description>30104</description>
      </extension>
      <extension name=".mpd">
        <description>30105</description>
      </extension>
      <extension name=".rmt">
        <description>30106</description>
      </extension>
      <extension name=".tmc">
        <description>30107</description>
      </extension>
      <extension name=".tm8">
        <description>30108</description>
      </extension>
      <extension name=".tm2">
        <description>30109</description>
      </extension>
      <extension name=".cms">
        <description>30110</description>
      </extension>
      <extension name=".cm3">
        <description>30111</description>
      </extension>
      <extension name=".dlt">
        <description>30112</description>
      </extension>
    </support>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>@PLATFORM@</platform>
    <license>GPL-2.0-or-later</license>
    <source>https://github.com/xbmc/audiodecoder.asap</source>
    <assets>
      <icon>icon.png</icon>
    </assets>
    <summary lang="ca_ES">ASAP - un altre reproductor Atari lleuger (Another Slight Atari Player)</summary>
    <summary lang="da_DK">ASAP - Another Slight Atari Player</summary>
    <summary lang="de_DE">ASAP - Another Slight Atari Player</summary>
    <summary lang="en_GB">ASAP - Another Slight Atari Player</summary>
    <summary lang="es_ES">ASAP - Otro Reproductor Ligero de Atari</summary>
    <summary lang="es_MX">ASAP - Otro Reproductor Ligero de Atari</summary>
    <summary lang="eu_ES">ASAP - Beste Atari erreproduzitzaile arin bat (Another Slight Atari Player)</summary>
    <summary lang="fi_FI">ASAP - Another Slight Atari Player</summary>
    <summary lang="fr_FR">Bientôt - Un autre petit lecteur Atari</summary>
    <summary lang="hr_HR">ASAP - Another Slight Atari Player</summary>
